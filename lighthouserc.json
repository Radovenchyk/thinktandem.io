{
  "ci": {
    "collect": {
      "url": [
        "http://thinkthandem.lndo.site/",
        "http://thinkthandem.lndo.site/work/poets-redesign/",
        "http://thinkthandem.lndo.site/blog/2019/09/19/still-on-drupal-6-here-are-your-options/"
      ],
      "numberOfRuns": 2,
      "staticDistDir": "./public"
    },
    "assert": {
      "preset": "lighthouse:recommended",
      "assertions": {
        "first-contentful-paint": [
          "warn",
          {
            "maxNumericValue": 2500,
            "aggregationMethod": "optimistic"
          }
        ],
        "interactive": [
          "warn",
          {
            "maxNumericValue": 5000,
            "aggregationMethod": "optimistic"
          }
        ],
        "uses-long-cache-ttl": "off",
        "uses-http2": "off"
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
